<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <stroke android:width="4dp" android:color="@color/primary_background"/>
            <solid android:color="@color/rounded_button_background"/>
        </shape>
    </item>
    <item android:state_pressed="true"
        android:drawable="@drawable/ic_new_spending">
        <shape android:shape="oval">
            <stroke android:width="4dp" android:color="@color/primary_background"/>
            <solid android:color="@color/rounded_button_background_pressed"/>
        </shape>
    </item>
</selector>